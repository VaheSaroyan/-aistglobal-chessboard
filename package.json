{
  "name": "@aistglobal/chessboard",
  "version": "1.0.8",
  "description": "Simple chess board",
  "keywords": [
    "chess",
    "chess board",
    "chess.js",
    "chessboard.js",
    "javascript"
  ],

  "files": [
    "dist/*.css",
    "dist/*.js"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify index.js -o dist/index.js",
    "watch": "watchify index.js -o dist/index.js -v",
    "scss-compile": "node-sass -rw src/scss -o dist"
  },
  "author": "Vahe Saroyan",
  "license": "MIT",
  "dependencies": {
    "@chrisoakman/chessboardjs": "^1.0.0",
    "browserify": "^16.5.0",
    "chess.js": "^0.10.2",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babelify": "^10.0.0",
    "node-sass": "^4.13.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  }
}
